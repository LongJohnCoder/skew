testJS("
inline int fn() foo(int x) {
  return => x;
}
var x = 0;
var y = foo(x++);
var z = foo(x++);
", "
function foo(x) {
  return function() {
    return x;
  };
}
var x = 0;
var y = foo((x = x + 1 | 0) - 1 | 0);
var z = foo((x = x + 1 | 0) - 1 | 0);
");
